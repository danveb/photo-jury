{
  "name": "photo-jury",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:danveb/photo-jury.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "sass-compile": "sass --watch --no-source-map src/main.scss src/style.css",
    "build:compile": "sass --no-source-map src/main.scss build/compiled.css",
    "build:prefixes": "postcss build/compiled.css --use autoprefixer --output build/prefixed.css",
    "build:optimize": "csso build/prefixed.css --output build/style.css"
  },
  "devDependencies": {
    "sass": "^1.69.5"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "csso-cli": "^4.0.2",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0"
  }
}
